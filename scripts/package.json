{
  "name": "claude-code-setup-assistant",
  "version": "1.0.0",
  "description": "Interactive setup assistant for Claude Code development projects",
  "main": "setup-assistant.js",
  "bin": {
    "setup": "./setup-assistant.js",
    "skeleton": "./skeleton-generator.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node setup-assistant.ts",
    "setup": "npm run build && node setup-assistant.js",
    "setup:dry-run": "npm run build && node setup-assistant.js --dry-run",
    "test": "jest --passWithNoTests",
    "lint": "eslint . --ext .ts,.js --fix",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "claude-code",
    "setup",
    "template",
    "automation"
  ],
  "author": "daktu32",
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^7.5.3",
    "chalk": "^4.1.2",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "inquirer": "^9.2.12",
    "mustache": "^4.2.0",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
